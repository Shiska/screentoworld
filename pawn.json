{
	"user": "Shiska",
	"repo": "screentoworld",
	"entry": "test.pwn",
    "output": "test.amx",
    "dependencies": [
        "sampctl/samp-stdlib"
    ],
    "dev_dependencies": [
        "pawn-lang/YSI-Includes",
        "Pottus/ColAndreas"
    ],
    "builds": [
        {
            "name": "amx",
            "args": [
                "-;+", "-(+", "-d3"
            ]
        },
        {
            "name": "compat"
        },
        {
            "name": "lst",
            "args": [
                "-l"
            ]
        },
        {
            "name": "asm",
            "args": [
                "-a"
            ]
        },
        {
            "name": "report",
            "args": [
                "-r"
            ],
            "input": "screentoworld.inc",
            "workingdir": "report"
        }
    ],
    "runtime": {
        "mode": "y_testing"
    }
}